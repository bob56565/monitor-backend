{
  "schema_version": "1.0.0",
  "gating_thresholds": {
    "minimum_data_windows_days": {
      "a1c_estimate": 30,
      "glucose_variability": 14,
      "bp_estimate": 7,
      "hrv_assessment": 7,
      "lipid_trend": 90,
      "inflammation_risk": 14,
      "default": 14
    },
    "minimum_sensor_quality": {
      "tight_range": 0.85,
      "wide_range": 0.70,
      "any_output": 0.50
    },
    "anchor_requirements": {
      "a1c_estimate_tight": ["recent_a1c_lab"],
      "a1c_estimate_wide": ["glucose_monitor_14d"],
      "lipid_tight": ["lipid_panel_90d"],
      "bp_tight": ["bp_readings_7d"],
      "default_tight": ["recent_lab_upload"]
    }
  },
  "confidence_parameters": {
    "max_confidence_measured": 95,
    "max_confidence_inferred_tight": 85,
    "max_confidence_inferred_wide": 70,
    "max_confidence_no_anchor": 55,
    "completeness_weights": {
      "specimen_uploads": 0.35,
      "isf_monitor_data": 0.25,
      "vitals_data": 0.20,
      "soap_profile": 0.20
    },
    "recency_decay_halflife_days": 90,
    "signal_quality_floor": 0.50,
    "modality_alignment_bonus": 0.10
  },
  "variability_bounds": {
    "glucose_cv_normal": 0.36,
    "hrv_cv_typical": 0.45,
    "bp_systolic_std_normal": 12.0,
    "bp_diastolic_std_normal": 8.0
  },
  "time_windows": {
    "recent_lab_days": 90,
    "stale_lab_days": 180,
    "vitals_recency_days": 30,
    "monitor_lookback_days": 60
  }
}
